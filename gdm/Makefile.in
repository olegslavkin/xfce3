PACKAGE = @PACKAGE@
VERSION = @VERSION@
XFCE_CONFDIR = @XFCE_CONFDIR@

SHELL = /bin/sh
@SET_MAKE@

srcdir = @srcdir@
top_srcdir = @top_srcdir@
VPATH = @srcdir@

prefix = @prefix@
exec_prefix = @exec_prefix@
bindir = @bindir@
gdm_prefix = @GDM_PREFIX@
switchdesk_prefix = "/usr/share/apps/switchdesk"
wmsession_prefix = "/etc/X11/wmsession.d"
subdir = gdm

ACLOCAL = @ACLOCAL@
AUTOCONF = @AUTOCONF@
AUTOMAKE = @AUTOMAKE@
AUTOHEADER = @AUTOHEADER@

INSTALL = @INSTALL@
INSTALL_PROGRAM = @INSTALL_PROGRAM@ $(AM_INSTALL_PROGRAM_FLAGS)
INSTALL_DATA = @INSTALL_DATA@
INSTALL_SCRIPT = @INSTALL_SCRIPT@

MKINSTALLDIRS = $(top_srcdir)/@MKINSTALLDIRS@

DEFS = @DEFS@

DISTFILES = Makefile.in XFce.in Xclients.xfce.in 10XFce.in

.SUFFIXES:

all: XFCE

XFCE:
	@sed -e "s%BIN_DIR%$(bindir)%g" \
	$(srcdir)/XFce.in > $(srcdir)/XFce
	@sed -e "s%BIN_DIR%$(bindir)%g" \
	$(srcdir)/10XFce.in > $(srcdir)/10XFce
	@sed -e "s%BIN_DIR%$(bindir)%g" \
	$(srcdir)/Xclients.xfce.in > $(srcdir)/Xclients.xfce

install: install-exec install-data
install-exec:
install-data: install-data-@USE_GDM@
install-data-no: all

install-data-yes: all
	if test -r "$(MKINSTALLDIRS)"; then \
	  $(MKINSTALLDIRS) $(DESTDIR)$(gdm_prefix); \
	  $(MKINSTALLDIRS) $(DESTDIR)$(switchdesk_prefix); \
	  $(MKINSTALLDIRS) $(DESTDIR)$(wmsession_prefix); \
	else \
	  $(SHELL) $(MKINSTALLDIRS) $(DESTDIR)$(gdm_prefix); \
	  $(SHELL) $(MKINSTALLDIRS) $(DESTDIR)$(switchdesk_prefix); \
	  $(SHELL) $(MKINSTALLDIRS) $(DESTDIR)$(wmsession_prefix); \
	fi
	$(INSTALL_SCRIPT) $(srcdir)/XFce $(DESTDIR)$(gdm_prefix)/XFce; \
	$(INSTALL_SCRIPT) $(srcdir)/Xclients.xfce $(DESTDIR)$(switchdesk_prefix)/Xclients.xfce;
	$(INSTALL_SCRIPT) $(srcdir)/10XFce $(DESTDIR)$(wmsession_prefix)/10XFce; \

installcheck:

uninstall:
	rm -f $(DESTDIR)$(gdm_prefix)/XFce
	rm -f $(DESTDIR)$(switchdesk_prefix)/Xclients.xfce
	rm -f $(DESTDIR)$(wmsession_prefix)/10XFce

check: all

dvi info tags TAGS ID:

mostlyclean:
	rm -f core core.*
	rm -fr *.bak

clean: mostlyclean

distclean: clean
	rm -f Makefile
	rm -f XFce
	rm -f 10XFce
	rm -f Xclients.xfce

maintainer-clean: distclean

Makefile:

# Tell versions [3.59,3.63) of GNU make not to export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:

